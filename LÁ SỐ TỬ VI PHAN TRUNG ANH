    <script>
        // Thêm hiệu ứng tương tác cho các sao
        document.querySelectorAll('.sao').forEach(sao => {
            sao.addEventListener('mouseenter', function() {
                const tooltip = document.createElement('div');
                tooltip.className = 'tooltip';
                
                // Nội dung tooltip tùy thuộc vào loại sao
                const saoText = this.textContent;
                let tooltipText = '';
                
                if (saoText.includes('Văn Khúc')) {
                    tooltipText = 'Văn Khúc - Chủ về văn học, nghệ thuật, trí tuệ. Bé có thiên hướng học giỏi văn chương, nghệ thuật.';
                } else if (saoText.includes('Hóa Kỵ')) {
                    tooltipText = 'Hóa Kỵ - Chủ về sự đố kỵ, hiểu lầm, trở ngại. Cần dạy bé tính cách cởi mở, tránh đa nghi.';
                } else if (saoText.includes('Tử Vi')) {
                    tooltipText = 'Tử Vi - Đế tinh, chủ về quyền lực, lãnh đạo. Bé có tố chất lãnh đạo, được mọi người yêu mến.';
                } else if (saoText.includes('Thiên Tướng')) {
                    tooltipText = 'Thiên Tướng - Tướng tinh, chủ về quản lý, hỗ trợ. Bé có khả năng quản lý, giúp đỡ người khác.';
                } else if (saoText.includes('Liêm Trinh')) {
                    tooltipText = 'Liêm Trinh - Chủ về sự trong sạch, công minh. Bé có tính cách ngay thẳng, trung thực.';
                } else if (saoText.includes('Thiên Cơ')) {
                    tooltipText = 'Thiên Cơ - Chủ về máy móc, tính toán, linh hoạt. Bé thông minh, nhanh nhẹn, có tư duy logic.';
                } else if (saoText.includes('Thái Dương')) {
                    tooltipText = 'Thái Dương - Chủ về quyền lực, danh vọng, sáng tạo. Bé có tính cách mạnh mẽ, sáng tạo.';
                } else if (saoText.includes('Thiên Lương')) {
                    tooltipText = 'Thiên Lương - Chủ về y tế, cứu giúp, nhân ái. Bé có lòng nhân hậu, thích giúp đỡ người khác.';
                } else {
                    tooltipText = 'Thông tin chi tiết về sao ' + saoText + ' trong tử vi.';
                }
                
                tooltip.textContent = tooltipText;
                this.appendChild(tooltip);
                
                // Định vị tooltip
                const rect = this.getBoundingClientRect();
                tooltip.style.left = '50%';
                tooltip.style.top = (rect.height + 5) + 'px';
                tooltip.style.transform = 'translateX(-50%)';
            });
            
            sao.addEventListener('mouseleave', function() {
                const tooltip = this.querySelector('.tooltip');
                if (tooltip) {
                    this.removeChild(tooltip);
                }
            });
        });
    </script>
</body>
